property defaultModuleName = 'Test01';
property outputDirectory = 'gen';

token Whitespace         : '\s' ;
token Identifier         : Letter LetterOrDigit* ;
token FixedToken         : 'token' ;
token Equal              : '==' ;
token NotEqual           : '!=' ;
token Not                : '!' ;
token Assign             : '=' ;

fragment LetterOrDigit   : Letter | Digit;
fragment Letter          : '\w' ;
fragment Digit           : '\d' ;
